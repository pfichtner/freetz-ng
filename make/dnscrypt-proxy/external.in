config EXTERNAL_FREETZ_PACKAGE_DNSCRYPT_PROXY
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_DNSCRYPT_PROXY
	bool "dnscrypt"
	default n
	help
		externals the following file(s):
		 /usr/bin/dnscrypt-proxy

config EXTERNAL_FREETZ_PACKAGE_DNSCRYPT_PROXY_CSV
	depends on EXTERNAL_FREETZ_PACKAGE_DNSCRYPT_PROXY
	bool "csv resolver file"
	default n
	help
		externals the following file(s):
                 /usr/share/dnscrypt-proxy/dnscrypt-resolvers.csv
